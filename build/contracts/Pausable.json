  "source": "pragma solidity ^0.4.6;\n\nimport \"./Owned.sol\";\n\ncontract Pausable is Owned {\n\n    bool public running;\n\n    modifier onlyIfRunning {require(running == true, \"cannot execute now\"); _;}\n\n    constructor()  public {\n        running = true;\n    }\n\n    function runSwitch(bool onOff) public onlyOwner returns(bool success) {\n        running = onOff;\n        return true;\n    }\n\n\n}",
